            <Label prefHeight="48.0" prefWidth="239.0" text="Create New Address Book">
               <font>
                  <Font name="Agency FB Bold" size="15.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="43.0" prefWidth="337.0" VBox.vgrow="NEVER">
               <children>
                  <JFXTextField fx:id="txt_bookName" prefHeight="43.0" prefWidth="258.0" promptText="Address Book Name" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="56.0" prefWidth="362.0">
               <children>
                  <JFXButton fx:id="btn_create" onAction="#createBook" prefHeight="42.0" prefWidth="94.0" text="Create Book" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
